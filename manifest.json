{
  "name": "Kick Volume Control",
  "description": "Using your scroll wheel change the volume on kick.com",
  "version": "2.0.0",
  "manifest_version": 3,

  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["*://*.kick.com/*"],
      "all_frames": true
    }
  ],
  "icons": {
    "128": "img/icon128.png"
  }
}
